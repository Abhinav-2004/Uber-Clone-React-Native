        "foregroundImage": "./assets/uber.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.abhinav_2004.uberclone"
    },
    "web": {
      "favicon": "./assets/uber.png"
    },
    "extra": {
      "eas": {
        "projectId": "399a745f-072a-420a-b56c-8a8c2cef4bd8"
      }
    },
    "plugins": [
      "./android-manifest.plugin.js"
    ]
  }
